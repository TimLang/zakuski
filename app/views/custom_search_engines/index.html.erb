<div class="container cse-container">
  <div class="row">
    <div class="span9">
      <div class="well">
      <% if @keeped_custom_search_engines.present? %>
        <% @keeped_custom_search_engines.each do |cse| %>
          <p><%= render partial: 'shared/cse_info', locals: {cse: cse} %></p>
        <% end %>
      <% else %>
        <div class="page-header">
          <h1><%= I18n.t('human.text.no_cses_header') %></h1>
        </div>
      <% end %>
      </div>
    </div>
    <div class="span3">
      <!-- cse dashboard start-->
      <%= render 'shared/dashboard' %>
      <!-- cse dashboard end --> 
    </div>
  </div>
</div>