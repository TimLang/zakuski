<%= form_for @custom_search_engine do |f| %>
<div class="row">
  <div class="span6 offset3">
  <%= render 'cse_specification' %>

  <div class="cse-divider span5"></div>
  
  <!-- access setting start-->
  <div class="control-group">
    <div class="controls">
    <%= f.label :access_public do %>
        <%= f.radio_button :access, 'public' %>
        <strong><%= I18n.t('human.text.public') %></strong><small class="cse-small"><%= I18n.t('human.text.public_what') %></small>
    <% end %>
    </div>
  </div>

  <div class="control-group">
    <div class="controls">
      <%= f.label :access_protected do %>
        <%= f.radio_button :access, 'protected' %>
        <strong><%= I18n.t('human.text.protected') %></strong><small class="cse-small"><%= I18n.t('human.text.protected_what') %></small>        
      <% end %>
    </div>
  </div>

  <div class="control-group">
    <div class="controls">
    <%= f.label :access_private do %>
      <%= f.radio_button :access, 'private' %>
      <strong><%= I18n.t('human.text.private')  %></strong><small class="cse-small"><%= I18n.t('human.text.private_what') %></small>      
    <% end %>
  </div>
  </div>
  <!-- access setting end -->
  <div class="cse-divider span5"></div>

  <%= render 'cse_annotations' %>

  <div class="control-group">
      <div class="controls">
        <%= f.submit I18n.t("human.controls.link.submit"), class: "btn btn-primary" %>
      </div>
      </div>
  </div>
</div>
<% end %>
