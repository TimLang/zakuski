<?xml version="1.0" encoding="UTF-8" ?>
<GoogleCustomizations>
<CustomSearchEngine language="zh-Hans">
  <Title><%= @custom_search_engine.specification.title %></Title>
  <Description><%= @custom_search_engine.specification.description %></Description>
  <Context>
    <BackgroundLabels>
      <Label name="_cse_filter" mode="FILTER" />
      <Label name="_cse_exclude" mode="ELIMINATE" />
      <Label name="_cse_boost" mode="BOOST" />
    </BackgroundLabels>
    <% if @facet_labels.present? %>
    <Facet>
    	<% @facet_labels.each do |label, modes| %>
		<% if modes.present? %>
			<% modes.each do |m| %>
					<FacetItem>
						<Label name="_cse_<%= label %>_<%= m %>" mode="<%= m.upcase %>"></Label>
						<% if m != 'exclude' %>
							<Title><%= label %></Title>
						<% else %>
							<Title/>
						<% end %>
					</FacetItem>
			<% end %>
		<% end %>
    	<% end %>
    </Facet>
	<% end %>
  </Context>
  <LookAndFeel code="4" nonprofit="false" element_layout="1" theme="7" custom_theme="false" text_font="Arial, sans-serif" url_length="full" enable_cse_thumbnail="true" promotion_url_length="full" ads_layout="1">
    <Logo />
    <Colors url="#009933" background="#FFFFFF" border="#FFFFFF" title="#1155CC" text="#333333" visited="#1155CC" title_hover="#1155CC" title_active="#1155CC" />
    <Promotions title_color="#1155CC" title_visited_color="#1155CC" url_color="#009933" background_color="#F6F6F6" border_color="#F6F6F6" snippet_color="#333333" title_hover_color="#1155CC" title_active_color="#1155CC" />
    <SearchControls input_border_color="#D9D9D9" button_border_color="#2F5BB7" button_background_color="#357AE8" tab_border_color="#CCCCCC" tab_background_color="#FFFFFF" tab_selected_border_color="#CCCCCC" tab_selected_background_color="#FFFFFF" />
    <Results border_color="#FFFFFF" border_hover_color="#FFFFFF" background_color="#FFFFFF" background_hover_color="#FFFFFF" />
  </LookAndFeel>
  <AdSense />
  <EnterpriseAccount />
  <autocomplete_settings />
</CustomSearchEngine>
<% if @custom_search_engine.annotations.present? %>
<Annotations>
  <% @custom_search_engine.annotations.each do |annotation| %>
    <Annotation about="<%= annotation.about %>" score="<%= 0.7+annotation.score.to_f/100 %>">
      <Label name="_cse_<%= annotation.mode %>"/>
      	<% if annotation.facet.present? %>
      		<Label name="_cse_<%= annotation.facet %>_<%= annotation.mode %>"/>		
      	<% end %>
    </Annotation>
  <% end %>
</Annotations>
<% end %>
</GoogleCustomizations>
