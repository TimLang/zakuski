		$('#cse-keep-btn').html("<%= I18n.t('human.text.keep_current_cse') %>").attr('href', "<%= keep_cse_path(@custom_search_engine) %>");
		$('#cse-consumers').find('#cse-consumer-<%= current_user.id%>').remove();
