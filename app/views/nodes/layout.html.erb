<div class="container">
	<div class="row">
		<div class="span7 offset1">
			<div class="cse-box-padding well">
			<div class="row-fluid">
				<%= render 'nodes/header' %>
			</div>
			<div class="row-fluid">
				<% if params[:post_type] == 'cses' %>
					<%= render 'cse_body' %>
				<% elsif params[:post_type] == 'topics' %>
					<%= render 'topic_body' %>
				<% end %>
			</div>
			<div class="row-fluid">
				<%= render 'nodes/footer' %>
			</div>
			</div>
		</div>
		<div class="span3">
			<%= render 'shared/dashboard' %>
		</div>
	</div>
</div>
