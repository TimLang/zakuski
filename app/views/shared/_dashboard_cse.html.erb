<li class="cse-link <%= 'cse-selected' if cse == @linked_cse %>" id="dashboard-cse-<%= cse.id %>">
	<%= link_to truncate(cse.title, length: 17 ), cse_path(cse), 
				title: cse.title %>
	 <div class="controls-row pull-right">
	  <a href="javascript:void(0)" class="link-cse" title="Link"><i class="icon-search icon-white"></i></a>
	  <%= hidden_field_tag :cseid, cse.id %>
	</div>
	<div class="hide">
	<span class="muted"><%= I18n.t('human.text.active_cse') %></span>
	<%= render partial: 'shared/cse_info', locals: {cse: cse} %>
	<div class="row-fluid">
		<%= render 'shared/cse_tags', cse: cse %>
		<%= render 'shared/cse_config', cse: cse %>
		<%= render 'shared/cse_description', cse: cse %>
	</div>
	</div>
</li>
